/* pages/scenicSpotBooking/index.wxss */
page {
  min-height: 100vh;
  background: url("http://47.108.86.202:8081/icon/bj01.png") no-repeat;
  background-size: 100%;
  background-position-y: -47rpx;
}
.nav_left{
  font-size: 40rpx;
  color: #000000;
  font-weight: 500;
  font-family: Source Han Serif CN;

}
.nav_left .nav_left_text{
  margin-left:24rpx;
}
/* 线上预约 */
.head {
  background: #fff;
  padding: 10rpx 0 0 30rpx;
  display: flex;
  height: 60rpx;
}
.margin {
  margin-left: 52rpx;
}
.appointment_title {
  position: relative;
  font-size: 28rpx;
  font-family: Source Han Serif CN, Source Han Serif CN-Medium;
  font-weight: 500;
  text-align: left;
  color: #333333;
}
.appointment_text {
  display: inline-block;
  position: absolute;
  width: 28rpx;
  height: 4rpx;
  left: 50%;
  transform: translateX(-50%);
  bottom: 10rpx;
  background: linear-gradient(180deg,#f0d78d, #e1cc8f);
  border-radius: 2rpx;
}
.top_padding {
  padding: 20rpx 30rpx 0 30rpx;
}
.top {
  padding: 54rpx 62rpx 0 48rpx;
  height: 462rpx;
  background: #ffffff;
  border-radius: 8rpx;
  box-shadow: 0px 10rpx 20rpx 0px rgba(130,130,130,0.16); 
  margin: 0 auto;
}
.top .top_title {
  font-size: 28rpx;
  font-family: Source Han Serif CN, Source Han Serif CN-Heavy;
  font-weight: 800;
  text-align: left;
  color: #1d1d1d;
}
.top .top_title image {
  width: 6rpx;
  height: 30rpx;
  border-radius: 0px 6rpx 6rpx 0px;
  margin-right: 26rpx;
}
.top_cell {
  margin-top: 28rpx;
}
.van-cell:after {
  left: 0 !important;
  right: 0 !important;
}
.top .top_cell .title_class {
  font-size: 28rpx;
  font-family: Source Han Serif CN, Source Han Serif CN-Medium;
  font-weight: 500;
  text-align: left;
  color: #646464;
}
.top .top_cell .value_class {
  font-size: 32rpx;
  font-family: Source Han Serif CN, Source Han Serif CN-Medium;
  font-weight: 500;
  text-align: right;
  color: #1d1d1d;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
}
.top .top_cell .ash {
  color: #C8C8C8;
}
.van-cell {
  padding: 34rpx 0 !important;
}
.middle {
  margin-top: 30rpx;
  /* height: 536rpx; */
  padding-bottom: 30rpx;
  background: #ffffff;
  border-radius: 8rpx;
  box-shadow: 0px 10rpx 20rpx 0px rgba(130,130,130,0.16); 
}
.middle_title {
  padding: 64rpx 0 0 48rpx;
  font-size: 28rpx;
  font-family: Source Han Serif CN, Source Han Serif CN-Heavy;
  font-weight: 800;
  text-align: left;
  color: #1d1d1d;
}
.middle_title image {
  width: 6rpx;
  height: 30rpx;
  margin-right: 26rpx;
}
.middle_card {
  padding: 48rpx 72rpx 0 72rpx;
  display: flex;
  flex-wrap: wrap;
}
.middle_card_white {
  margin-right: 34rpx;
  width: 160rpx;
  height: 60rpx;
  background: #ffffff;
  border: 2rpx solid #f0d78d;
  border-radius: 30rpx;
  font-size: 22rpx;
  font-family: Source Han Serif CN, Source Han Serif CN-Medium;
  font-weight: 500;
  line-height: 60rpx;
  text-align: center;
  color: #1d1d1d;
  margin-bottom: 20rpx;
}
.background_yellow {
  background: linear-gradient(180deg,#f0d78d, #e1cc8f);
}
.middle_in_box {
  width: 100%;
  margin-top: 138rpx;
  display: flex;
  justify-content: center;
}
.middle_in {
  width: 400rpx;
  height: 80rpx;
  background: #ffffff;
  border: 2rpx solid #f0d78d;
  border-radius: 40rpx;
  font-size: 28rpx;
  color: #E7D08E;
  line-height: 80rpx;
  text-align: center;
}
.list_box {
  width: 100%;
  font-size: 24rpx;
  text-align: center;
  color: #e7d08e;
  margin-top: 42rpx;
}
.bottom_box {
  /* position: absolute; */
  /* bottom: -140rpx; */
  margin: 50rpx auto;
  display: flex;
  justify-content: center;
  /* left: 50%; */
  /* transform: translateX(-50%); */
}
.bottom {
  display: flex;
}
.bottom_left {
  width: 280rpx;
  height: 80rpx;
  background: linear-gradient(180deg,#ff8080, #ff6969);
  border-radius: 40rpx 0px 0px 40rpx;
  box-shadow: 0px -20rpx 40rpx 0px rgba(184,184,184,0.1); 
}
.bottom_right {
  width: 280rpx;
  height: 80rpx;
  background: linear-gradient(180deg,#f0d78d, #e1cc8f);
  border-radius: 0px 40rpx 40rpx 0px;
  box-shadow: 0px -20rpx 40rpx 0px rgba(184,184,184,0.1); 
}
.bottom_title {
  font-size: 28rpx;
  font-family: Source Han Serif CN, Source Han Serif CN-Medium;
  font-weight: 500;
  text-align: center;
  color: #ffffff;
  line-height: 80rpx;
}
