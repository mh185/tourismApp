/* pages/index/selectedFiveDistricts/index.wxss */
page {
  min-height: 100vh;
  background: url("http://47.108.86.202:8081/icon/bj01.png") no-repeat;
  background-size: 100%;
  background-position-y: -47rpx;
}
.nav_left{
  font-size: 40rpx;
  color: #000000;
  font-weight: 500;
  font-family: Source Han Serif CN;
}
.nav_left .nav_left_text{
  margin-left:24rpx;
}
/* .scroll-view_H{
  padding: 26px 17px 0;
  overflow: scroll;
  box-sizing: Border-box;
} */
.nav {
  display: flex;
}
.list_box {
  background: #fff;
  padding: 28rpx 0 28rpx 30rpx;
}
.districts {
  /* width: 86rpx; */
  height: 80rpx;
  font-size: 32rpx;
  font-family: Source Han Serif CN, Source Han Serif CN-Medium;
  font-weight: 500;
  line-height: 80rpx;
  /* text-align: left; */
  color: #1d1d1d;
  background: url(http://47.108.86.202:8081/icon/hill.png) no-repeat;
  background-position: 5rpx;
  background-size: 86rpx 80rpx;
}
.scenic_box {
  width: 330rpx;
  margin-right: 20rpx;
}
.img_box {
  position: relative;
  width: 330rpx;
  height: 200rpx;
}
.img_box image {
  width: 330rpx;
  height: 200rpx;
  border-radius: 8rpx;
}
.aaa {
  position: absolute;
  top: 0;
  width: 120rpx;
  height: 44rpx;
  font-size: 24rpx;
  background: linear-gradient(180deg,#f0d78d, #e1cc8f);
  border-radius: 8rpx 0px 16rpx 0px;  
  font-family: Source Han Serif CN, Source Han Serif CN-Medium;
  font-weight: 500;
  text-align: center;
  line-height: 44rpx;
  color: #552f09;
}
.title {
  margin-top: 18rpx;
  font-size: 28rpx;
  font-family: Source Han Serif CN, Source Han Serif CN-Medium;
  font-weight: 500;
  text-align: left;
  color: #1d1d1d;
}
.time {
  font-size: 22rpx;
  font-family: Source Han Serif CN, Source Han Serif CN-Medium;
  font-weight: 500;
  text-align: left;
  color: #c8c8c8;
  margin-top: 10rpx;
}