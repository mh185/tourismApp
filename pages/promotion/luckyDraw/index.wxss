/* pages/promotion/luckyDraw/index.wxss */

.lucky-wrap {
    background: #6ACBA5;
    min-height: 100vh;
}

.relative {
    position: relative;
}

.topbar {
    display: flex;
    align-items: stretch;
    justify-content: space-between;
    position: absolute;
    top: 34rpx;
    left: 24rpx;
    right: 24rpx;
    height: 50rpx;
    line-height: 50rpx;
}

.topbar-score {
    font-size: 30rpx;
    font-style: italic;
    font-weight: bold;
    letter-spacing: 1rpx;
    color: #fff;
}

.topbar-record {
    width: 162rpx;
    height: 50rpx;
    font-size: 22rpx;
    letter-spacing: 1rpx;
    color: rgba(255,255,255,.8);
    text-align: center;
    background: url("data:image/png;base64,") center no-repeat;
    background-size: contain;
}

.showMsg {
    width: 750rpx;
    height: 586rpx;
}

.showText {
    width: 510rpx;
    height: 220rpx;
    position: absolute;
    top: 112rpx;
    left: 0;
    right: 0;
    margin: auto;
}

.lucky-roulette {
    padding: 0 20rpx;
}

.head-text {
    text-align: center;
    transform: translateY(-55rpx);
}

.head-text__img {
    width: 518rpx;
    height: 96rpx;
}

.foot-text {
    padding-bottom: 72rpx;
    margin-top: 58rpx;
    text-align: center;
}

.foot-text__img {
    width: 202rpx;
    height: 60rpx;
}

.roll-list {
    width: 432rpx;
    height: 46rpx;
    line-height: 46rpx;
    padding: 0 32rpx;
    margin-left: 15rpx;
    background :#000;
    opacity: 0.2;
    border-radius: 8rpx;
    box-sizing: border-box;
}

.roll-list__item {
    font-size: 22rpx;
    color: #fff;
}

.roulette {
    width: 697rpx;
    height: 597rpx;
    padding: 39rpx;
    margin: 16rpx auto 0;
    background: url("data:image/png;base64,") no-repeat 100% 100%;
    box-sizing: border-box;
}

.lucky_item {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 175rpx;
    height: 142rpx;
    margin: -6rpx -10rpx;
    background: url("data:image/png;base64,") center no-repeat;
    background-size: cover;
}

.lucky_item--active {
    background-image: url("data:image/png;base64,")
}

.goods {
    width: 134rpx;
    height: 105rpx;
    line-height: 0.9;
    text-align: center;
}

.goods-img {
    max-width: 100rpx;
    max-height: 66rpx;
}

.goods-name {
    font-size: 22rpx;
    color: #673410;
    margin-top: 3rpx;
}

.roulette-row {
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.roulette-row--reverse {
    flex-flow: row-reverse;
}

.roulette-column {
    flex: 1;
    display: flex;
    flex-direction: column;
}

.roulette-column--reverse {
    flex-flow: wrap-reverse;
}

.roulette-button {
    flex: 2;
    position: relative;
    display: flex;
    align-items: center;
    justify-content: center;
}

.start {
    width: 263rpx;
    height: 172rpx;
}

.surplus {
    position: absolute;
    bottom: 38rpx;
    font-size: 24rpx;
    color: #fff;
}

.popover {
    position: fixed;
    z-index: 2;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    display: flex;
    flex-direction: column;
    opacity: 0;
    background: rgba(0,0,0,.8);
    transition: opacity .2s;
    pointer-events: none;
}

.popover-bg {
    flex: 1;
}

.popover-wrap {
    padding: 36rpx 30rpx;
    background: #fff;
    transform: translateY(100%);
    transition: transform .2s;
}

.popover--show {
    opacity: 1;
    pointer-events: auto;
}

.popover--show .popover-wrap {
    transform: translateY(0);
}

.popover-head {
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.popover-head .title {
    font-size: 36rpx;
    color: #111;
}

.popover-head .close {
    width: 36rpx;
    height: 36rpx;
}

.popover-content {
    height: 710rpx;
    line-height: 2;
    font-size: 22rpx;
    margin-top: 40rpx;
    color: #111;
}

.rule-content .part {
    margin-bottom: 30rpx;
}

.rule-content .indent {
    padding-left: 15rpx;
}

.record-list {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 24rpx 6rpx;
    border-bottom: 1rpx solid #E0E0E0;
}

.record-list .list-item {
    font-size: 24rpx;
    line-height: 1.6;
}

.record-list .list-item__name {
    color: #B9B9B9;
}

.record-list .list-item__value {
    color: #111;
}